 <tag>
   <name>auth</name>
   <tag-class>org.whale.system.auth.tag.AuthTag</tag-class>

	<attribute>
	 	<name>authCode</name>
	 	<required>true</required>
	 	<rtexprvalue>true</rtexprvalue>
	</attribute>
	 <attribute>
		 <name>authAdmin</name>
		 <required>false</required>
		 <rtexprvalue>false</rtexprvalue>
	 </attribute>
	 <attribute>
		 <name>authLogin</name>
		 <required>false</required>
		 <rtexprvalue>false</rtexprvalue>
	 </attribute>
 </tag>


 <tag>
   <name>dict</name>
   <tag-class>org.whale.system.tag.DictTag</tag-class>

   <attribute>
     <name>id</name>
     <required>true</required>
     <rtexprvalue>true</rtexprvalue>
   </attribute>
   
    <attribute>
     <name>headerLabel</name>
     <required>false</required>
     <rtexprvalue>true</rtexprvalue>
   </attribute>
   
   <attribute>
     <name>width</name>
     <required>false</required>
     <rtexprvalue>true</rtexprvalue>
   </attribute>

   <attribute>
     <name>dictCode</name>
     <required>true</required>
     <rtexprvalue>true</rtexprvalue>
   </attribute>

	<attribute>
     <name>valDefault</name>
     <required>false</required>
     <rtexprvalue>true</rtexprvalue>
     <description>默认显示值</description>
   </attribute>

   <attribute>
     <name>value</name>
     <required>false</required>
     <rtexprvalue>true</rtexprvalue>
     <description>默认选中值</description>
   </attribute>

   <attribute>
     <name>readonly</name>
     <required>false</required>
     <rtexprvalue>true</rtexprvalue>
   </attribute>
   
   <attribute>
     <name>disabled</name>
     <required>false</required>
     <rtexprvalue>true</rtexprvalue>
   </attribute>
   
   <attribute>
     <name>sql</name>
     <required>false</required>
     <rtexprvalue>true</rtexprvalue>
     <description>数据集由sql获取，注意默认 option值在sql中的key：SQL_VALUE_KEY, option文本在sql中的key:SQL_LABEL_KEY</description>
   </attribute>
   
   <attribute>
     <name>sqlValueKey</name>
     <required>false</required>
     <rtexprvalue>true</rtexprvalue>
     <description>用户自定义 option值在sql中的key</description>
   </attribute>
   
   <attribute>
     <name>sqlLableKey</name>
     <required>false</required>
     <rtexprvalue>true</rtexprvalue>
     <description>用户自定义 option文本在sql中的key</description>
   </attribute>
   <attribute>
     <name>formatJsonObj</name>
     <required>false</required>
     <rtexprvalue>true</rtexprvalue>
     <description>打印字典js对象</description>
   </attribute>
 </tag>
 
