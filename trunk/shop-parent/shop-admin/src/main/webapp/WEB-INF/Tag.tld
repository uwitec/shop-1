 <tag>
 	<name>image</name>
   	<tag-class>org.whale.system.tag.ImageTag</tag-class>
   	<description>图片上传处理控件</description>
   	
   	<attribute>
	  <name>id</name>
	  <required>true</required>
	  <rtexprvalue>true</rtexprvalue>
	  <description>ID</description>
	</attribute>
	<attribute>
	  <name>value</name>
	  <required>false</required>
	  <rtexprvalue>true</rtexprvalue>
	  <description>初始值</description>
	</attribute>
	<attribute>
	  <name>url</name>
	  <required>false</required>
	  <rtexprvalue>true</rtexprvalue>
	  <description>上传地址</description>
	</attribute>
	<attribute>
	  <name>fileTypeExts</name>
	  <required>false</required>
	  <rtexprvalue>true</rtexprvalue>
	  <description>上传控件类型限制</description>
	</attribute>
	<attribute>
	  <name>maxNum</name>
	  <required>false</required>
	  <rtexprvalue>true</rtexprvalue>
	  <description>图片上传的最多数量</description>
	</attribute>
	<attribute>
	  <name>fileSizeLimit</name>
	  <required>false</required>
	  <rtexprvalue>true</rtexprvalue>
	  <description>文件大小限制</description>
	</attribute>
	<attribute>
	  <name>readonly</name>
	  <required>false</required>
	  <rtexprvalue>true</rtexprvalue>
	  <description>只能查看，不能删除，编辑</description>
	</attribute>
	<attribute>
	  <name>saveWay</name>
	  <required>false</required>
	  <rtexprvalue>true</rtexprvalue>
	  <description>保存到ftp的服务器地址</description>
	</attribute>
	<attribute>
	  <name>dirName</name>
	  <required>false</required>
	  <rtexprvalue>true</rtexprvalue>
	  <description>保存文件相对路径</description>
	</attribute>
	<attribute>
	  <name>extInfo</name>
	  <required>false</required>
	  <rtexprvalue>true</rtexprvalue>
	  <description>用户附加数据</description>
	</attribute>
 </tag>
 
